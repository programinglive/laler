{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laler/laler",
    "description": "Capture PHP dumps and route them to configurable channels. Works with or without Laravel.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "dump",
        "debug",
        "debugging"
    ],
    "require": {
        "php": "^8.0",
        "psr/container": "^1.0|^2.0",
        "symfony/var-dumper": "^6.0|^7.0"
    },
    "suggest": {
        "illuminate/contracts": "For Laravel integration (^9.0|^10.0|^11.0|^12.0)",
        "illuminate/support": "For Laravel integration (^9.0|^10.0|^11.0|^12.0)"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.5|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Laler\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Laler\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laler\\LalerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
